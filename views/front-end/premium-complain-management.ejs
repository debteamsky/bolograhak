<script>
jQuery(function ($) {
   $('body').on('submit', '.level_form', function(e){
      e.preventDefault();
      

       const formData = $(this).serializeArray();
      // //const formData = new FormData(Data);
      //const formData = new FormData($(this)[0]);
      const jsonData = {};
      // $(formData).each(function (index, field) {
      //       jsonData[field.name] = field.value;
      // });
      $(formData).each(function(index, field) {
         if (jsonData[field.name] === undefined) {
            // If the property doesn't exist in jsonData, create an array and add the value
            jsonData[field.name] = [field.value];
         } else {
            // If the property already exists, push the value to the existing array
            jsonData[field.name].push(field.value);
         }
      });
      // const repeaterData = {};
      // $(this).find('input').each(function () {
      //    repeaterData[this.name] = $(this).val();
      // });
      // const formData = [];

      // $(this).each(function() {
      // const repeaterData = {};
      // $(this).find('input').each(function() {
      //    repeaterData[this.name] = $(this).val();
      // });
      // formData.push(repeaterData);
      // })
      console.log(jsonData);
      //return false;

      $.ajax({
         url: '/auth/create-company-level',
         method: 'POST',
         data: formData,
         processData: false,
         contentType: false,
         //ContentType: 'application/json',
         success: function (data) {
            if (data.status == 'ok') {
            $('.submit-loading').hide();
            $('#create_category_form')[0].reset();
            $('.submit-success').text(data.message);
            $('.submit-success').show();
            $('#submit_btn').prop( "disabled", false );
            setTimeout( function(){
               location.reload(true);
            }, 3000 );
            } else {
               $('.submit-loading').hide();
            $('.submit-error').text(data.message);
            $('.submit-error').show();
               $('#submit_btn').prop( "disabled", false );
            }
            //console.log(data);
         },
         error: function (error) {
            $('.submit-loading').hide();
            $('.submit-error').text(error);
            $('.submit-error').show();
            $('#submit_btn').prop( "disabled", false );
         }
      });
   })
})
</script>